    <static_object name="Stage">
        <origin xyz="0.0 0.0 -0.349" rpy="0 0 0" />
        <visual>
            <origin xyz="0.0 0.0 0.0" rpy="0 0 0" />
            <geometry>
                <box size="10 12 0.01" />
            </geometry>
            <material name="wood_material">
                <!-- Use wood.png texture for the material -->
                <texture filename="/Users/rheamalhotra/Desktop/robotics/optitrack-robot-dance/optitrack/assets/wood.png"/>
            </material>
        </visual>
    </static_object>

    <light name="light1" type="directional">
        <position xyz="2.0 2.0 2.0" />
        <lookat xyz="0.0 0.0 0.0" />
    </light>

    <light name="light2" type="directional">
        <position xyz="-2.0 -2.0 2.0" />
        <lookat xyz="0.0 0.0 0.0" />
    </light>

    <camera name="camera_fixed">
        <position xyz="4.0 -4.0 8.0" />
        <vertical xyz="0.0 0.0 20.0" />
        <lookat xyz="4.0 0.0 0.0" />
    </camera>

</world>
