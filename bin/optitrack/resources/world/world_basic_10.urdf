<?xml version="1.0" ?>
<world name="demo_world" gravity="0.0 0.0 -3.0">

    <!-- Define Toro robots in a vertical line along the y-axis -->
    <robot name="toro0">
        <model dir="./resources" path="./model/toro.urdf" name="toro0" />
        <origin xyz="1.0 -4.5 -1.3" rpy="0 0 0" />
    </robot>
    <robot name="toro1">
        <model dir="./resources" path="./model/toro.urdf" name="toro1" />
